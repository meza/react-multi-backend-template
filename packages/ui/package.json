{
  "name": "@meza/react-template-ui",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack -p",
    "ci": "yarn test && yarn cpd && yarn build:prod",
    "ci:dev": "yarn test && yarn cpd && yarn build",
    "check:types": "tsc --noEmit",
    "cpd": "jscpd src",
    "clean": "rm -rf dist",
    "clean:all": "yarn clean && rm -rf node_modules reports",
    "start": "webpack-dev-server",
    "lint": "yarn lint:scss && yarn check:types && yarn lint:ts && yarn lint:components",
    "lint:fix": "yarn lint:scss:fix && yarn lint:ts:fix && yarn lint:components:fix",
    "lint:ts": "eslint --cache --cache-location ../../.cache/ui/.eslintcache --ext ts,tsx,js,jsx src webpack.config.babel.js",
    "lint:ts:fix": "eslint --cache --cache-location ../../.cache/ui/.eslintcache --fix --ext ts,tsx,js,jsx src webpack.config.babel.js",
    "lint:components": "node testUtils/checkMockComponents",
    "lint:components:fix": "node testUtils/generateMockComponents",
    "lint:scss": "stylelint --cache --cache-location ../../.cache/ui/.stylelintcache src",
    "lint:scss:fix": "stylelint --cache --cache-location ../../.cache/ui/.stylelintcache --fix src",
    "test": "yarn lint && jest --coverage",
    "test:no-cache": "yarn test --no-cache",
    "report": "open ./reports/coverage/lcov-report/index.html"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.5",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@hot-loader/react-dom": "^16.11.0",
    "@meza/jest-coverage-bumper": "^1.0.1",
    "@svgr/webpack": "^4.3.3",
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.3.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.4.7",
    "css-loader": "^3.3.2",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.3.0",
    "eslint-config-tailored-tunes": "^3.0.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "favicons-webpack-plugin": "^3.0.1",
    "file-loader": "^5.0.2",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-chance": "^0.1.8",
    "jscpd": "^2.0.16",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react-test-renderer": "^16.12.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^0.23.1",
    "stylelint": "^12.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.13.0",
    "stylelint-webpack-plugin": "^1.2.1",
    "terser-webpack-plugin": "latest",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "yarn": "^1.21.0"
  },
  "dependencies": {
    "@loadable/component": "^5.12.0",
    "@sentry/browser": "^5.11.2",
    "connected-react-router": "^6.6.1",
    "history": "^4.10.1",
    "normalize.css": "^8.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-localstorage": "^1.0.0-rc5"
  },
  "jscpd": {
    "output": "./reports/",
    "threshold": 10,
    "reporters": [
      "html",
      "console"
    ],
    "ignore": [
      "**/__snapshots__/**",
      "**/*.svg",
      "**/*.test.*"
    ],
    "absolute": true,
    "gitignore": true
  },
  "jest": {
    "testEnvironment": "node",
    "timers": "fake",
    "globalSetup": "jest-chance",
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.svg": "./scripts/transformToFliename.js"
    },
    "moduleNameMapper": {
      "\\.scss": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "d.ts"
    ],
    "coverageDirectory": "./reports/coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testMatch": [
      "**/?(*.)test.ts?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!**/*.d.ts",
      "!**/definitions/*.*",
      "!test/**/*",
      "!src/index.tsx",
      "!src/redux/store.ts",
      "!src/redux/reducers/index.ts"
    ],
    "reporters": [
      "default"
    ],
    "testResultsProcessor": "@meza/jest-coverage-bumper",
    "cacheDirectory": "../../.cache/jest/ui"
  }
}
